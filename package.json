{
  "name": "mock-system",
  "version": "1.0.0",
  "main": "electron-main.js",
  "homepage": "./",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "electron": "electron .",
    "electron-dev": "NODE_ENV=development electron .",
    "electron-clean": "./start-electron-clean.sh",
    "build": "electron-builder",
    "build-mac": "electron-builder --mac",
    "build-mac-pre": "CSC_IDENTITY_AUTO_DISCOVERY=false electron-builder --mac",
    "build-win": "electron-builder --win",
    "build-linux": "electron-builder --linux",
    "dist": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Mock System Team",
  "license": "MIT",
  "description": "一个强大的API Mock和代码生成工具",
  "dependencies": {
    "@prisma/client": "^6.15.0",
    "@xiaobaidadada/node-pty-prebuilt": "^1.0.5",
    "axios": "^1.10.0",
    "body-parser": "^2.2.0",
    "chokidar": "^4.0.3",
    "diff-match-patch": "^1.0.5",
    "dotenv": "^17.0.0",
    "express": "^5.1.0",
    "js-yaml": "^4.1.0",
    "mockjs": "^1.1.0",
    "multer": "^2.0.1",
    "openai": "^5.8.2",
    "prismjs": "^1.30.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-parser": "^10.0.3",
    "swagger-ui-express": "^5.0.1",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@electron/rebuild": "^3.7.2",
    "electron": "^28.0.0",
    "electron-builder": "^24.6.4",
    "electron-rebuild": "^3.2.9",
    "prisma": "^6.10.1"
  },
  "build": {
    "appId": "com.mock-system.app",
    "productName": "Mock System",
    "directories": {
      "output": "dist"
    },
    "files": [
      "**/*",
      "!node_modules/**/*",
      "node_modules/**/*",
      "public/**/*",
      "prisma/**/*",
      "!**/*.md",
      "!**/*.log",
      "!**/test/**/*",
      "!**/tests/**/*",
      "!**/.git/**/*",
      "!**/.gitignore",
      "!**/package.json.backup",
      "!**/test-server.js",
      "!**/start-electron-clean.sh",
      "!**/fix-*.js",
      "!**/fix-*.sh",
      "!**/ultimate-*.js",
      "!**/ultimate-*.sh",
      "!**/final-*.js",
      "!**/final-*.sh",
      "!**/simple-*.sh",
      "!**/solve-*.sh",
      "!**/complete-*.sh",
      "!**/rewrite-*.js",
      "!**/MACOS_BUILD_SUCCESS.md",
      "!**/INSTALLATION_GUIDE.md",
      "!**/README.md",
      "!**/.DS_Store",
      "!**/Thumbs.db",
      "!**/node_modules/.cache/**/*",
      "!**/node_modules/.bin/**/*",
      "!**/node_modules/**/test/**/*",
      "!**/node_modules/**/tests/**/*",
      "!**/node_modules/**/docs/**/*",
      "!**/node_modules/**/examples/**/*",
      "!**/node_modules/**/demo/**/*",
      "!**/node_modules/**/sample/**/*",
      "!**/node_modules/**/samples/**/*",
      "!**/node_modules/**/benchmark/**/*",
      "!**/node_modules/**/benchmarks/**/*",
      "!**/node_modules/**/coverage/**/*",
      "!**/node_modules/**/nyc_output/**/*",
      "!**/node_modules/**/dist/test/**/*",
      "!**/node_modules/**/dist/tests/**/*",
      "!**/node_modules/**/lib/test/**/*",
      "!**/node_modules/**/lib/tests/**/*",
      "!**/node_modules/**/src/test/**/*",
      "!**/node_modules/**/src/tests/**/*"
    ],
    "extraResources": [
      {
        "from": "node_modules/.prisma",
        "to": "node_modules/.prisma",
        "filter": [
          "**/*"
        ]
      },
      {
        "from": "prisma",
        "to": "prisma",
        "filter": [
          "**/*"
        ]
      }
    ],
    "mac": {
      "category": "public.app-category.developer-tools",
      "target": [
        {
          "target": "dmg",
          "arch": [
            "x64",
            "arm64"
          ]
        }
      ],
      "icon": "assets/icon.icns",
      "hardenedRuntime": false,
      "gatekeeperAssess": false,
      "notarize": false,
      "signIgnore": [
        "node_modules/@xiaobaidadada/node-pty-prebuilt/build/Release/pty.node"
      ]
    }
  }
}